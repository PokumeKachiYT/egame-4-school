[sub_resource type="GDScript" id=14]
resource_name = "AddButton"
script/source = "extends Panel

var tween = Tween.new()
onready var root = get_parent()
export var toggled: bool = false

onready var button = get_node(\"RotationHelper/Button\")

func on_click() -> void:
	if root.paused:
		return
	
	tween.remove(get_node(\"Background\"),'rect_size:x')
	tween.remove(get_node(\"RotationHelper\"),'rotation_degrees')
	tween.interpolate_property(
		get_node(\"RotationHelper\"),'rotation_degrees',
		get_node(\"RotationHelper\").rotation_degrees,0 if toggled else 360,
		.35,Tween.TRANS_CIRC,Tween.EASE_OUT
	)
	tween.interpolate_property(
		get_node(\"Background\"),'rect_size:x',
		get_node(\"Background\").rect_size.x,100 if toggled else 500,
		.45,Tween.TRANS_EXPO,Tween.EASE_OUT
	)
	tween.start()
	
	toggled = not toggled

func on_hover(is_hovering) -> void:
	pass

func _ready() -> void:
	add_child(tween)
	button.connect('mouse_entered',self,'on_hover',[true])
	button.connect('mouse_exited',self,'on_hover',[false])
	button.connect('pressed',self,'on_click')
"

